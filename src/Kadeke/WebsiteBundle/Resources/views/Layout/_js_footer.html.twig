
    {% javascripts
        '@KadekeWebsiteBundle/Resources/public/js/jquery.js'
        '@KadekeWebsiteBundle/Resources/public/js/jquery.infinitescroll.min.js'
        '@KadekeWebsiteBundle/Resources/public/js/bootstrap.min.js'
        '@KadekeWebsiteBundle/Resources/public/js/bootswatch.js'

        filter='yui_js'
        output='frontend/footer.js'
    %}
        <script src='{{ asset_url }}'></script>
    {% endjavascripts %}
    <script type="text/javascript" src="http://balupton.github.com/jquery-syntaxhighlighter/scripts/jquery.syntaxhighlighter.min.js"></script>
    <script type="text/javascript">
        $.SyntaxHighlighter.init({
            'theme':'google',
            'themes': ['google']
        });
        jQuery('.blog').infinitescroll({
            navSelector  : "div.blognav",
            // selector for the paged navigation (it will be hidden)
            nextSelector : "div.blognav a:first",
            // selector for the NEXT link (to page 2)
            itemSelector : "div.day",
            // selector for all items you'll retrieve
            loading: {
                finishedMsg: "",
                msgText: ""
            }
        });
    </script>